        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:text="hello"
        android:layout_weight="1"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="28dp" />
</LinearLayout>

